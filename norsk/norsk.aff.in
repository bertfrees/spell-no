    [AIOUYØÅ]      >       INGEN           # gå     > gåingen
    [AIOUYØÅ]      >       INGENS          # gå     > gåingens
    [AIOUYØÅ]      >       INGA            # gå     > gåinga

flag u:       # -lig-endelse i verb. Se også flagg Z.
    [^LM]          >       LIG             # blå     > blålig
    [^LM]          >       LIGE            # blå     > blålige
    L              >       IG              # adel    > adelig
    L              >       IGE             # adel    > adelige
    M              >       MELIG           # rom     > rommelig
    M              >       MELIGE          # rom     > rommelige


flag *v:       # -else-endelse i verb
    E              >       LSE             # ane    > anelse
