    [IOÅ]          >       ELSEN           # tilstå > tilståelsen
    [IOÅ]          >       ELSENS          # tilstå > tilståelsens

flag *w:       # endelse: -het
    .              >       HET             # glatt > glatthet
    .              >       HETEN           # glatt > glattheten
    .              >       HETENS          # glatt > glatthetens

flag *x:       # endelse: -skap
