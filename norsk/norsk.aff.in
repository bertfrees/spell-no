# Affikstabell for norsk
# Skrevet av Ivar Aavatsmark, oktober 1993.
# Sterkt utvidet av Rune Kleveland mai 1998. (kanskje for sterkt)
# I filen local.h er satt:
#
#   #define MASKBITS        64
#

compoundwords controlled z  #Skal ALDRI brukes med compoundwords on.
compoundmin 4
allaffixes on
flagmarker /

defstringtype "list" "nroff" ".list"

boundarychars [".,;:]  #Dette brukes av Babel for LaTeX for å støtte koding
                       #av for eksempel vi"llede for rett deling vill-lede.
                       #`.' gjør at man finner steder der man har glemt
                       #mellomrom etter . og ,.
stringchar î Î
wordchars [a-c] [A-C]
stringchar ç Ç
wordchars [d-e] [D-E]
stringchar é É
stringchar ê Ê
stringchar è È
wordchars [f-o] [F-O]
stringchar ó Ó
stringchar ô Ô
stringchar ò Ò
wordchars [p-z] [P-Z]
stringchar æ Æ
stringchar ø Ø
stringchar å Å

altstringtype "tex" "TeX"  ".tex" ".bib"

altstringchar  ç ç 
altstringchar  Ç Ç
altstringchar  é é
altstringchar  É É
altstringchar  ê ê
altstringchar  Ê Ê
altstringchar  è è
altstringchar  È È
altstringchar  ó ó
altstringchar  Ó Ó
