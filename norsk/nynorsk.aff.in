# Her er en oversikt over benyttede flagg:
#
# Brukt:         ABCDEFGHIJKLMNOPQRSTUVWXYZ[\]^_`abcdefghijklmnopqrstuvwxy
#
# Tilgjengelig: z
#
# z kan brukes til å kontrollere sammensatte ord.


prefixes

# Ekte forstavelser:


flag *a:
    .              >       U               # ferdig > uferdig

flag *b:
    .              >       FORÎ            # dampe > fordampe

flag *c:
    .              >       GJENÎ           # vinne > gjenvinne

flag *d:
    .              >       HOVUDÎ          # sak > houedsak

flag *e:
    .              >       MIS             # tenke > mistenke

flag *f:
    .              >       SAM             # arbeid > samarbeid

flag *g:       # forstavelse: in-
    [^BLMPR]       >       INÎ             # effektiv > ineffektiv
    L              >       ILÎ             # legal    > illegal
    [BMP]          >       IM              # mobil    > immobil
    R              >       IR              # regulær  > irregulær

# Ikke behandlet er forstavelser som: an-, be-, er-, re-.
#

# Forstavelser som er preposisjoner eller adverb:
#

flag *h:
    .              >       ETTERÎ          # leve > etterleve

flag *i:
    .              >       TILÎ            # stå > tilstå

flag *j:
    .              >       OVERÎ           # tid > overtid

flag *k:
    .              >       UNDERÎ          # kjenne > underkjenne

flag *l:
    .              >       PÅÎ             # føre > påføre

flag *m:
    .              >       AVÎ             # stå > avstå

flag *n:
    .              >       INNÎ            # føre > innføre

flag *o:
    .              >       UTÎ             # føre > utføre

flag *p:
    .              >       OPPÎ            # stå > oppstå

flag *q:
    .              >       NEDÎ            # bryte > nedbryte

flag *r:
    .              >       OMÎ             # gå > omgå

flag *s:
    .              >       SAMANÎ          # sett > samansett


suffixes

# Avledningsendelser:
#


flag *t:       # -ing-endelse i verb
    E              >       -E,ING          # vandre > vandring
#   E              >       -E,INGEN        # vandre > vandringen
#   E              >       -E,INGENS       # vandre > vandringens
    E              >       -E,INGA         # vandre > vandringa
    [AIOUYØÅ]      >       ING             # gå     > gåing
#   [AIOUYØÅ]      >       INGEN           # gå     > gåingen
#   [AIOUYØÅ]      >       INGENS          # gå     > gåingens
    [AIOUYØÅ]      >       INGA            # gå     > gåinga

flag u:       # -lig-endelse i verb. Se også flagg Z.
    [^LM]          >       LEG             # blå     > blålig
    [^LM]          >       LEGE            # blå     > blålige
    L              >       EG              # adel    > adelig
    L              >       EGE             # adel    > adelige
    M              >       MELEG           # rom     > rommelig
    M              >       MELEGE          # rom     > rommelige







flag *x:       # endelse: -skap
    [^E]           >       SKAP            # klok  > klokskap
    E              >       -E,SKAP         # regne > regnskap

flag *y:       # endelse: -itet + ning (Ja, de er ortogonale)
    [AEIOUYØ] [DLMNRSV]  >   ITET          # normal  > normalitet
    [AEIOUYØ] [DLMNRSV]  >   ITETEN        # normal  > normaliteten
    E L L          >       -ELL,ALITET     # formell > formalitet
    E L L          >       -ELL,ALITETEN   # formell > formaliteten
    Æ R            >       -ÆR,ARITET      # regulær > regularitet
    Æ R            >       -ÆR,ARITETEN    # regulær > regulariteten
    E              >       -E,NING         # lade   > ladning
    E              >       -E,NINGEN       # lade   > ladningen


